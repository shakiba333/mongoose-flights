<h2>All Flights</h2>
<table>
    <thead>
        <tr>
            <th>airline</th>
            <th>airport</th>
            <th>flightNo</th>
            <th>departs</th>
        </tr>
    </thead>
    <tbody>
        <% flights.forEach(function(f){ %>
            <tr>
                <td>
                    <%= f.airline %>
                </td>
                <td>
                    <%= f.airport %>
                </td>
                <td>
                    <%= f.flightNo %>
                </td>
                <td>
                    <%= f.departs %>
                </td>
            </tr>
            <% }) %>
    </tbody>
</table>